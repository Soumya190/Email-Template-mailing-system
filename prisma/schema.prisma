generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}


model EmailTemplate {
  id        String   @id @default(uuid())
  name      String   @unique
  subject   String
  body      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


model Contact {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  group     String?
  createdAt DateTime @default(now())
}

model Activity {
  id        String   @id @default(uuid())
  action    String
  createdAt DateTime @default(now())
}